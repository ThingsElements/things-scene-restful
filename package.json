{
  "name": "@things-scene/restful",
  "version": "0.2.1",
  "description": "Restful Client Component for Things Scene",
  "things-scene": true,
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-restful.js src/index.js & polymer serve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-restful.js && uglifyjs things-scene-restful.js -o things-scene-restful-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-restful.git"
  },
  "author": "",
  "license": "http://things-scene.hatiolab.com/EULA/",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-restful/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-restful#readme",
  "devDependencies": {
    "@hatiolab/things-shell": "^0.3.0"
  },
  "dependencies": {
    "@hatiolab/things-scene": "^2.2.36"
  }
}
